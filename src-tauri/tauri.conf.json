{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run dev",
		"devPath": "http://localhost:3000",
		"distDir": "../out"
	},
	"package": {
		"productName": "tobylearn",
		"version": "1.1.5"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"dialog": {
				"all": true
			},
			"http": {
				"all": false,
				"request": true,
				"scope": [
					"https://raw.githubusercontent.com/Zendard/TobyLearn/*"
				]
			}
		},
		"bundle": {
			"active": true,
			"category": "Education",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "org.zendard.tobylearn",
			"longDescription": "A simple, easy learning helper which allows you to create sets and let the program question you to help you learn for tests etc.",
			"resources": [],
			"shortDescription": "A simple learning helper made with tauri",
			"targets": [
				"appimage",
				"deb",
				"nsis"
			]
		},
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "tobylearn",
				"width": 800
			}
		]
	}
}